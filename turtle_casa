from turtle import *

shape("classic") #tipo de seta
speed(5) #velocidade da seta

color("Red") # cor da sete
pensize(1) #grossura da seta
forward(1) #direita
right(50)
backward(120)
left(100)
backward(115)

left(40)
backward(100)
right(0)
backward(100)
right(90)
forward(300)
left(90)
forward(200)
right(0)
forward(1)
right(90)
backward(150)

